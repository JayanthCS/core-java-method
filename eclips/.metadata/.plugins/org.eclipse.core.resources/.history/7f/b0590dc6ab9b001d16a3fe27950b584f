package com.xworkz.details;

public class Information {

	public static void main(String[] args) {
	
		public String name(String name1) {
			
			System.out.println("Inside name method");
			System.out.println("name is "+ name1);
			
			String degree = "BE";
			System.out.println("Degree is "+ degree);
			System.out.println("Outside name method ");
		}

	}

}
